.overlay {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: #fff;
}
.overlay__wrap-icon {
    height:auto;
    float: left;
    margin-bottom:10px;
}

.overlay__ad {
    position: absolute;
    background-color: #000;
}

.overlay__icon {
    width: 34px;
    margin: 0px auto;
    display: block;
}

.overlay__copy {
    float:left;
    font-size: 100%;
    text-align: center;
    color: #fff;
    text-transform: uppercase;
	line-height:normal;
}

.overlay__ad-alternate {
    position: absolute;
    left: 50%;
}

.overlay__element {
    width: auto;
    height: auto;
    float: left;
    display: none;
}

.overlay__element-bug {
    width: 38px;
    height: 38px;
}

.overlay__wrap_msg {
    position:absolute;
    top: 35%;
	left:0%;
    width:100%;
    margin:0px auto;
}

.overlay__element-bug img {
    margin:5px 0px 0px 5px;
    width: 60px;
    height: 41px;
}

.overlay__element-options {
    width: 50px;
    height: 100px;
}

.overlay__element-like {
    background-color: rgba(0,0,0,0.7);
}

.overlay__element-settings {
    background-color: rgba(0,0,0,0.7);
}

.overlay__option {
    float: left;
    width: 35px;
    height: 35px;
}

.overlay__option img {
    width:100%;
	height:100%;
}

.overlay__element-dislike {
    background-color: rgba(0,0,0,0.7);
}

.overlay__stage {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 100001;
    font-family: sans-serif;
}

.overlay__stage--bug .overlay__element-bug {
    display: block;
}

.overlay__stage--options .overlay__element-options {
    display: block;
}

.overlay__stage--like .overlay__element-like {
    display: block;
}

.overlay__stage--settings .overlay__element-settings {
    display: block;
}

.overlay__stage--dislike .overlay__element-dislike {
    display: block;
}

.overlay__stage--choice .overlay__element-choice stage {
    display: block;
}

.overlayCopySmall {
    width: 100%;
    font-size: 100%;
    transform: none;
    margin-top: 10px;
}
