CLOSE #1
END SUB

SUB ShowGuy (xshow%, yshow%, kindofput%, itemtoshow%)
amountlossx% = xshow% - 1: amountlossy% = yshow% - 1
IF kindofput = 1 THEN
 FOR x% = 1 TO 25: FOR y% = 1 TO 20
	PSET (x% + amountlossx%, y% + amountlossy%), guy%(itemtoshow%, x%, y%)
 NEXT y%: NEXT x%
ELSE
 FOR x% = 1 TO 25: FOR y% = 1 TO 20
	IF guy%(itemtoshow%, x%, y%) <> 0 THEN
	 PSET (x% + amountlossx%, y% + amountlossy%), guy%(itemtoshow%, x%, y%)
	END IF
 NEXT y%: NEXT x%
END IF
END SUB

